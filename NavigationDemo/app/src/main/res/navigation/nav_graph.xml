<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/firstNavFragment">

    <fragment
        android:id="@+id/firstNavFragment"
        android:name="ar.edu.ort.navigationdemo.FirstNavFragment"
        android:label="fragment_first_nav"
        tools:layout="@layout/fragment_first_nav" >
        <action
            android:id="@+id/action_firstNavFragment_to_secondNavigationFragment"
            app:destination="@id/secondNavigationFragment" />
    </fragment>
    <fragment
        android:id="@+id/secondNavigationFragment"
        android:name="ar.edu.ort.navigationdemo.secondNavigationFragment"
        android:label="fragment_second_navigation"
        tools:layout="@layout/fragment_second_navigation" />
</navigation>